<antlib>
  <typedef name="library" classname="jetbrains.mps.build.ant.ModuleJarDataType" />
  <typedef name="jvmarg" classname="jetbrains.mps.build.ant.Arg" />
  <typedef name="jvmargs" classname="jetbrains.mps.build.ant.JvmArgs" />
  <typedef name="macro" classname="jetbrains.mps.build.ant.Macro" />

  <taskdef name="migrate" classname="jetbrains.mps.build.ant.MigrationTask" />

  <!--used only in generated build scripts-->
  <taskdef name="generate" classname="jetbrains.mps.build.ant.generation.GenerateTask" />
  <taskdef name="gentest" classname="jetbrains.mps.build.ant.generation.GenTestTask" />
  <taskdef name="copyModels" classname="jetbrains.mps.build.ant.converter.ConvertToBinaryTask" />
  <taskdef name="runMPS" classname="jetbrains.mps.build.ant.generation.MpsRunnerTask" />

  <typedef name="repository" classname="jetbrains.mps.build.ant.RepositoryDataType" />
  <typedef name="folder" classname="jetbrains.mps.build.ant.ModulesFolderDataType" />
  <typedef name="module" classname="jetbrains.mps.build.ant.ModuleDataType" />
  <typedef name="all-mps-modules" classname="jetbrains.mps.build.ant.AllModulesDataType" />

  <typedef name="chunk" classname="jetbrains.mps.build.ant.generation.Chunk" />
